<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OrcaStarsWebApplication</title>
    @if (IsSectionDefined("Styles"))
    {
        @RenderSection("Styles", required: true)
    }
    else
    {
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" media="all" />
    }
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.12.4.js"></script>
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg bg-light">
            <ul class="navbar-nav">
                <li class="nav-item"><a class="nav-link" asp-action="Index" asp-controller="Admin">Index</a></li>
                <li class="nav-item"><a class="nav-link" asp-action="Form" asp-controller="Admin">Form</a></li>
                <li class="nav-item"><a class="nav-link" asp-action="Search" asp-controller="Admin">Search</a></li>
                <li class="nav-item"><partial name="_LoginPartial" /></li> 
            </ul>
        </nav>
    </header>
    <div class="body-html">
        @RenderBody()
    </div>
    <!-- Enables use to call and use Scripts as needed in our views -->
    @if (IsSectionDefined("Scripts"))
    {
        @RenderSection("Scripts", required: true)
    }
</body>
</html>
